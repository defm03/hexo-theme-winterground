<%- partial('_partial/head') %>

<body>
  <%- partial('_partial/experiment') %>
  <header id="header" class="inner"><%- partial('_partial/header') %></header>
  <div id="content" class="inner">
    <% if ( page.layout == 'page' ){ %>
      <div id="main-col-page"><div id="wrapper-page"><%- body %></div></div>
    <% } else { %>
      <div id="main-col" class="alignleft"><div id="wrapper"><%- body %></div></div>
      <aside id="sidebar" class="alignright"><%- partial('_partial/sidebar') %></aside>
    <% } %>
    <div class="clearfix"></div>
  </div>
  <footer id="footer" class="inner"><%- partial('_partial/footer') %></footer>
  <%- partial('_partial/after_footer') %>
  <%- partial('_partial/bds') %>
  <link rel="stylesheet" href="<%- config.root %>bxslider/jquery.bxslider.css" type="text/css">
  <script src="<%- config.root %>bxslider/jquery.bxslider.min.js"></script>  
  <script src="<%- config.root %>js/experiment.js"></script>
</body>
</html>
